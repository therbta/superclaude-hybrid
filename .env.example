# ============================================
# SuperClaude Hybrid Configuration
# ============================================
# Copy this file to .env and fill in your values
# cp .env.example .env

# Core API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ZAI_API_KEY=your_zai_api_key_here
DEFAULT_PROVIDER=claude

# MCP Server Configuration
SEQUENTIAL_THINKING_ENABLED=true
CONTEXT7_ENABLED=true
PLAYWRIGHT_ENABLED=true
CHROME_DEVTOOLS_ENABLED=true
TAVILY_ENABLED=false
TAVILY_API_KEY=your_tavily_api_key
SERENA_ENABLED=false
MAGIC_ENABLED=false
TWENTYFIRST_API_KEY=your_21st_dev_api_key
MORPH_ENABLED=false
MORPH_API_KEY=your_morph_api_key

# SuperClaude Plugin Configuration
PM_AGENT_ENABLED=true
PM_AGENT_CONFIDENCE_THRESHOLD=0.9
RESEARCH_ENABLED=true
RESEARCH_DEPTH=standard
INDEX_REPO_ENABLED=true

# Behavioral Modes
DEFAULT_MODE=orchestration

# Paths and Directories
CLAUDE_DIR=${HOME}/.claude
LOG_DIR=./logs
BACKUP_DIR=${HOME}/.claude/config-backups

# Advanced Configuration
HOT_RELOAD_ENABLED=true
SAFE_MODE=true
API_TIMEOUT_MS=300000
DEBUG=false
